{
  "manifest_version": 2,
  "name": "fakenewsbot",
  "version": "0.1",
  "background": {
    "scripts": []
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.4.1.min.js", "post.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": ["tabs", "<all_urls>", "storage"]
}
